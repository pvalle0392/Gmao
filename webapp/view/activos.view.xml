<mvc:View xmlns:core="sap.ui.core" 
		  xmlns:mvc="sap.ui.core.mvc" 
		  xmlns="sap.m" 
		  controllerName="ebimList.ListBinding.controller.activos"
		  xmlns:html="http://www.w3.org/1999/xhtml">


		<App>
		<pages>
			<Page title="Activos">
				<content>
					<Label text="Ubicación Técnica: " class="sapUiResponsiveMargin"/>
					<Input id="ub-tec" width="250px"/>
					<Button text="Buscar" icon="sap-icon://search" press="onPress3" id ="boton" />
					<List id="equi-list" headerText="Activos encontrados" items="{/items}">
						<items>
							<ObjectListItem
							title="{Zdescript}"
							number="{Zemplaza}"
							intro="{Zequnr}"
							>
							</ObjectListItem>
						</items>
						
						
					</List>
				</content>
			</Page>
		</pages>
	</App>

</mvc:View>